#include <stdio.h>
#include <stdlib.h>
#include <math.h>  // estou acumulando bibliotecas
#include <time.h>
//exercício 3 página 118
int main() {
    int n, num, i, j;
    long long fat; // para suportar valores grandes

    printf("Quantos numeros deseja ler? ");
    scanf("%d", &n);
    for (i = 1; i <= n; i++) {
        printf("Digite um numero: ");
        scanf("%d", &num);
        fat = 1;
        for (j = 1; j <= num; j++) {
            fat = fat * j;}
        printf("Numero: %d\tFatorial: %lld\n", num, fat);}
    return 0;}
